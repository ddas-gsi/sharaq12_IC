							S12_X_I_C2[2][k][m] = S12_X_I_C[2][k][m] * (S12_X_factor / list_SRPPAC_gain2[1][int(S12_Y_I_C[0][0][l] / 4)][5][int(S12_X_I_C[0][k][m])]);
