									S12_Y_I_C[1][l][m] = S12_Y_I[1][l][m];
									S12_Y_I_C[2][l][m] = S12_Y_I[2][l][m] * (S12_Y_factor / list_SRPPAC_gain[0][5][int(S12_Y_I[0][l][m])]);
								}
							}
						}

